var shot_id  = "<%= @invite.shot_id %>"
$("#" + shot_id).fadeOut(600);

$("#accept" + shot_id).hide(600);
$("#delete" + shot_id).hide(600);

var new_accepted_shot = $("<%= j render 'shots/small_shot', shot: @invite.shot%>").hide();
$('#my_current_shots').prepend(new_accepted_shot);
new_accepted_shot.show(1000);


