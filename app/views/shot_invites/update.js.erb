var invite_id  = "<%= @invite.shot_id %>"
$("#" + invite_id).fadeOut(600);

$("#accept" + invite_id).hide(600);
$("#delete" + invite_id).hide(600);

var new_accepted_shot = $("<%= j render 'shots/small_shot', shot: @invite.shot%>").hide();
$('#accepted').prepend(new_accepted_shot);
new_accepted_shot.fadeIn(1000);


